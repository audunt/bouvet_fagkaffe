@inject IHttpContextAccessor httpContextAccessor
@page "/claims"
@{
    var user = httpContextAccessor.HttpContext?.User;
}

<PageTitle>Claims</PageTitle>

<h1>Claims</h1>

@foreach (var claim in user?.Claims ?? [])
{
    <text>
        Key: @claim.Type <br />
        Value: @claim.Value
        <br />
        <br />
    </text>
}
